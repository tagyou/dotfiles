{
    "editor.fontSize": 14,
    "editor.fontFamily": "Ricty",
    "editor.lineHeight": 16,
    "editor.wordWrap": "on",
    "editor.wrappingIndent": "same",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight":1.1,
    "vim.searchHighlightColor": "rgba(0, 200, 0, 0.5)",
    "vim.hlsearch": true,
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j","k"],
            "after": ["<Esc>"]
        }
    ],
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.openPositioning": "last",
    "workbench.colorTheme": "Atom One Dark",
    "workbench.activityBar.visible": true,
    "workbench.colorCustomizations": {
      "tab.activeBackground": "#555555cc",
    },

    "python.linting.flake8Enabled": true,
    "python.linting.pylintEnabled": false,
    "python.formatting.provider": "autopep8",
    "python.linting.flake8Args": [
        "--max-line-length",
        "120",
        "--ignore=E266,W503",
    ],
    "python.formatting.autopep8Args": [
      "--max-line-length",
      "88",
      "--aggressive",
      "--aggressive",
    ],
    "explorer.confirmDelete": false,
    "vsnotes.defaultNotePath": "~/Notes",
    "vsnotes.noteTitleConvertSpaces": " ",
    "vsnotes.defaultNoteTitle": "{dt}_{title}.{ext}",
    "vsnotes.tokens": [
      {
        "type": "datetime",
        "token": "{dt}",
        "format": "YYYY-MM-DD",
        "description": "Insert formatted datetime."
      },
      {
        "type": "title",
        "token": "{title}",
        "description": "Insert note title from input box.",
        "format": "Untitled"
      },
      {
        "type": "extension",
        "token": "{ext}",
        "description": "Insert file extension.",
        "format": "md"
      }
    ],
    "[markdown]":  {
      "editor.wordWrap": "on",
      "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
      },
      "editor.snippetSuggestions": "top"
      },
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.opacity": 0.8,
    "go.formatTool": "goimports",
    "git.autofetch": true,
    "yaml.schemaStore.enable": false,
    "terminal.integrated.scrollback": 4000,
    "terminal.integrated.shell.osx": "/bin/bash",
    "workbench.editorAssociations": {
      "*.ipynb": "jupyter-notebook"
    },
    "redhat.telemetry.enabled": true,
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.cursorSurroundingLines": 5,
    "notebook.cellToolbarLocation": {
      "default": "right",
      "jupyter-notebook": "left"
    },
    "git.ignoreLegacyWarning": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "[python]": {
      "editor.formatOnType": true
    },
}
